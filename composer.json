{
    "name": "andrey-helldar/cashier-tinkoff-qr",
    "description": "Driver for payment with QR codes via Tinkoff (see andrey-helldar/cashier)",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "bank",
        "billing",
        "cashier",
        "tinkoff",
        "qr"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/cashier-tinkoff-qr/issues",
        "source": "https://github.com/andrey-helldar/cashier-tinkoff-qr"
    },
    "require": {
        "php": "^7.3|^8.0",
        "andrey-helldar/cashier-tinkoff-auth": "^1.3.1",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "andrey-helldar/cashier": "^1.1.1",
        "andrey-helldar/support": "^4.1",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "nesbot/carbon": "^2.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\CashierDriver\\Tinkoff\\QrCode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
